//Passing 1D array

#include<stdio.h>

#define N 5

void print_array(int *ptr)
{
        printf("Array elements are : ");
        for(int i=0;i<N;i++)
        {
                printf("%d ", *(ptr+i));
        }
        printf("\n");
}

int main()
{
        int arr[N] = {39, 20, 10, 30, 80};
        print_array(arr);
}
